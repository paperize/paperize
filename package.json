{
  "name": "editor",
  "version": "0.0.5",
  "description": "The Paperize serverless web application",
  "main": "lib/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",

    "start": "npx webpack-dev-server --hot --host localhost --config webpack.dev.js",
    "start-test": "npx webpack-dev-server --host localhost --config webpack.test.js --env.NO_SYNC=true",
    "server": "http-server build",

    "dev-cypress": "NODE_ENV=test concurrently --kill-others \"npm run start-test\" \"cypress open\"",
    "build-test-integration": "npm run clean && NODE_ENV=test npx webpack --config webpack.dev.js",
    "test-integration": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run\"",
    "test": "npm run test-integration",

    "clean": "rm -rf build",
    "build-prod": "npm run clean && npx webpack --config webpack.prod.js",
    "deploy-prod": "coffee deploy.coffee",
    "bump-prod": "npm run build-prod && npm run deploy-prod",
    "release": "npm test && npm run build-prod && npm run deploy-prod",

    "clean-screenshots": "rm -rf cypress/screenshots/documentation_screenshots_spec.coffee",
    "generate-screenshots": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run --spec cypress/integration/documentation_screenshots_spec.coffee\"",
    "copy-screenshots": "cp cypress/screenshots/documentation_screenshots_spec.coffee/*.png ../docs/static/generated-images",
    "bump-screenshots": "npm run clean-screenshots && npm run generate-screenshots && npm run copy-screenshots"
  },
  "author": "Loren Norman <loren@oilandrope.com>",
  "license": "ISC",
  "devDependencies": {
    "@mdi/font": "^3.4.93",
    "@mdi/js": "^3.4.93",
    "@vue/test-utils": "^1.0.0-beta.25",
    "ajv": "^6.12.3",
    "aws-sdk": "^2.1033.0",
    "cloc": "^2.3.4",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.0.0",
    "concurrently": "^3.5.0",
    "core-util-is": "^1.0.2",
    "css-loader": "^3.5.3",
    "cypress": "^9.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-vue": "^4.3.0",
    "express": "^4.17.1",
    "file-api": "^0.10.4",
    "file-loader": "^3.0.1",
    "http-server": "^0.12.3",
    "mime-types": "^2.1.14",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.0.0",
    "recursive-readdir": "^2.2.1",
    "semver": "^5.3.0",
    "util-deprecate": "^1.0.2",
    "vite": "^2.9.9",
    "vite-plugin-vue2": "^2.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bluebird": "^3.5.0",
    "color-parse": "^1.3.8",
    "embed-js": "^4.2.1",
    "event-emitter": "^0.3.4",
    "idb-keyval": "^3.1.0",
    "jspdf": "github:lorennorman/jspdf#paperize-0.6.x",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "promisify-file-reader": "^3.0.7",
    "uuid": "^3.4.0",
    "vue": "^2.5.21",
    "vue-async-computed": "^3.3.1",
    "vue-color": "^2.7.0",
    "vue-observe-visibility": "^0.4.3",
    "vue-router": "^3.0.1",
    "vue-upload-component": "^2.8.20",
    "vuedraggable": "^2.16.0",
    "vuetify": "^1.5.2",
    "vuex": "^3.0.1"
  }
}
